<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.lenpdok.mapper.UserMapper">
    <select id="findByUsername" resultType="com.example.lenpdok.model.User">
        SELECT
            *
        FROM
            user
        where
            user_id = #{username}
            and password = #{password}
    </select>
</mapper>